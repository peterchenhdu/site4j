<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.github.peterchenhdu.site4j.biz.mapper.BizArticleTagsMapper">
    <resultMap id="rm" type="com.github.peterchenhdu.site4j.biz.entity.BizArticleTags">
        <result property="id" jdbcType="BIGINT" column="id"/>
        <result property="tagId" jdbcType="BIGINT" column="tag_id"/>
        <result property="articleId" jdbcType="BIGINT" column="article_id"/>
        <result property="createTime" jdbcType="TIMESTAMP" column="create_time"/>
        <result property="updateTime" jdbcType="TIMESTAMP" column="update_time"/>
    </resultMap>
    <sql id="Base_Column_List">
        id,
        tag_id,
        article_id,
        create_time,
        update_time
    </sql>

    <select id="query" parameterType="com.github.peterchenhdu.site4j.biz.dto.req.ArticleTagQueryDto"
            resultMap="rm">
        SELECT
        t.*
        FROM
        biz_article_tags t
        WHERE
        1 = 1
    </select>
</mapper>

